{
  "name": "xdevkit-view-compiler",
  "version": "0.9",
  "main": "src/app.js",
  "repository": "git@github.com:/autoaim-jp/xdevkit-view-compiler.git",
  "author": "autoaim_jp <git@autoaim.jp>",
  "scripts": {
		"compile": "node ./src/app.js --js ./view/src/js/ --css ./view/src/css/ --ejs ./view/src/ejs/page/ --ejs-component ./view/src/ejs/component/ --out ./view/build/ --ejs-config ../view/src/config/ejs.config.js --tailwindcss-config ./view/src/config/tailwind.config.cjs --tailwindcss-file ./view/src/css/tailwind.css --js-ignore _lib,_setting --command compile",
		"compile-minify": "node ./src/app.js --js ./view/src/js/ --css ./view/src/css/ --ejs ./view/src/ejs/page/ --ejs-component ./view/src/ejs/component/ --out ./view/build/ --ejs-config ../view/src/config/ejs.config.js --tailwindcss-config ./view/src/config/tailwind.config.cjs --tailwindcss-file ./view/src/css/tailwind.css --js-ignore _lib,_setting --command compile --minify",
		"watch": "node ./src/app.js --js ./view/src/js/ --css ./view/src/css/ --ejs ./view/src/ejs/page/ --ejs-component ./view/src/ejs/component/ --out ./view/build/ --ejs-config ../view/src/config/ejs.config.js --tailwindcss-config ./view/src/config/tailwind.config.cjs --tailwindcss-file ./view/src/css/tailwind.css --js-ignore _lib,_setting --command watch"
  },
  "type": "module",
  "dependencies": {
    "axios": "^0.27.2",
    "clean-css": "^5.3.2",
    "commander": "^10.0.1",
    "connect-redis": "^6.1.3",
    "ejs": "^3.1.9",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "html-minifier": "^4.0.0",
    "ioredis": "^5.2.2",
    "jest": "^29.5.0",
    "js-beautify": "^1.14.7",
    "uglify-js": "^3.17.4"
  },
  "devDependencies": {
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.3",
    "jsdoc": "^3.6.11",
    "tui-jsdoc-template": "^1.2.2"
  }
}
